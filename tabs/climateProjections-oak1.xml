<tab id="climateProjections-oak1">
    <title>Climate Projections</title>
    <timeslider />
    <mugloverrides>
      <element name="plotarea">
        <attribute name="marginright" value="35" />
      </element>
      <element name="horizontalaxis">
        <attribute name="min" value="1990" />
        <attribute name="max" value="2100" />
        <element name="binding">
          <attribute name="id" value="Tab3TimeBinding" />
        </element>
        <element name="pan">
          <attribute name="min" value="1900" />
          <attribute name="max" value="2100" />
        </element>
        <element name="zoom">
          <attribute name="min" value="6M" />
        </element>
      </element>
      <element name="verticalaxis[id=tempf]">
        <element name="pan">
          <attribute name="allowed" value="yes" />
          <attribute name="min"     value="-1" />
          <attribute name="max"     value="10" />
        </element>
        <element name="zoom">
          <attribute name="allowed" value="yes" />
          <attribute name="min"     value="1" />
          <attribute name="max"     value="50" />
        </element>
      </element>
      <element name="verticalaxis[id=tempf]">
        <element name="title" text="Temperature Change from 1971-1999 Average (&#730;F)"/>
      </element>
      <element name="verticalaxis[id=temp]">
        <element name="title" text="Temperature Change from 1971-1999 Average (&#730;C)"/>
        <element name="grid">
          <attribute name="visible" value="false" />
        </element>
      </element>
    </mugloverrides>
    <graphRef id="cp-oak1"/>
</tab>
