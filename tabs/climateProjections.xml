<tab container="Tab1SlotContainer">
    <title>Climate Projections</title>
    <timeslider />
    <mugloverrides>
      <element name="plotarea">
        <attribute name="marginright" value="35" />
      </element>
      <element name="horizontalaxis">
        <attribute name="min" value="1990" />
        <attribute name="max" value="2100" />
        <element name="binding">
          <attribute name="id" value="Tab3TimeBinding" />
        </element>
        <element name="pan">
          <attribute name="min" value="1900" />
          <attribute name="max" value="2100" />
        </element>
        <element name="zoom">
          <attribute name="min" value="6M" />
        </element>
      </element>
      <element name="verticalaxis">
        <element name="pan">
          <attribute name="allowed" value="yes" />
        </element>
      </element>
    </mugloverrides>
    <graphRef id="cp" />
    <legend>
      <item>
	<img src="/includes/dashboard/assets/ProjectionGraphLegendIcons-Red-width2.png"/>
	<text>High Growth (A2)</text>
      </item>
      <item>
	<img src="/includes/dashboard/assets/ProjectionGraphLegendIcons-Green-width2.png"/>
	<text>Moderate Growth (A1B)</text>
      </item>
      <item>
	<img src="/includes/dashboard/assets/ProjectionGraphLegendIcons-Blue-width2.png"/>
	<text>Low Growth (B1)</text>
      </item>
      <item>
	<img src="/includes/dashboard/assets/ProjectionGraphLegendIcons-Black-width2.png"/>
	<text>20th Century Climate</text>
      </item>
    </legend>
  </tab>
